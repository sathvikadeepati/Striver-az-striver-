time-o(n)
space-o(n)
class Solution{
   public static ListNode startingpointofcycle(ListNode head){
    HashSet<ListNode> hs=new HashSet<>();
    ListNode curr=head;
    while(curr!=null){
        if(hs.contains(curr)){
            return curr;
        }
        hs.add(curr);
        curr=curr.next;
   }
return null;
  }
}
optimal
time-o(n)
space-o(1)
class Solution{
 public static ListNode startingpointofcycle(ListNode head){
    ListNode slow=head;
    ListNode fast=head;
    while(fast!=null && fast.next!=null){
        slow=slow.next;
        fast=fast.next.next;
        if(slow==fast){
           slow=head;
           while(slow!=fast){
           slow=slow.next;
           fast=fast.next;
           }
           return slow;
        }
    }

 return null;
}
}
