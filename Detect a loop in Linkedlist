time-o(n)
space-o(n)
class Solution{
public static boolean Detectacycle(ListNode head){
  HashMap<ListNode,Integer> mp=new HashMap<>();
  ListNode curr=head;
  while(curr!=null){
       if(mp.containsKey(curr)){
          return true;
       }
       mp.put(curr,1);
       curr=curr.next;
  }
 return false;
}
}
optimal
time-o(n)
space-o(1)
class Solution{
   public static boolean detectacycle(ListNode head){
   ListNode slow=head;
   ListNode fast=head;
   while(fast!=null && fast.next!=null){
      slow=slow.next;
      fast=fast.next.next;
      if(slow==fast){
        return true;
      }
   }
return false;
}
}
