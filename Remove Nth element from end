time-o(n)
space-o(1)
class Solution{
  
  public static ListNode deletenthnodefromlast(ListNode head,int k){
    ListNode curr=head;
    int count=0;
    while(curr!=null){
      count+=1;
      curr=curr.next;
    }
    if(count==k){
       return head.next;
      }
    int len=count-k-1;
    curr=head;
    for(int i=0;i<len;i++){
      curr=curr.next;
    }
   curr.next=curr.next.next;
   return head;
  }
}
time-o(n)
space-O(1)
class Solution{
  
    public static ListNode deletenthnodefromlast(ListNode head,int k){
      ListNode slow=head;
      ListNode fast=head;
      for(int i=0;i<k;i++){
        fast=fast.next;
      }
      if(fast==null){
        return head.next;
      }
      while(fast.next!=null){
         slow=slow.next;
         fast=fast.next;
      }
     slow.next=slow.next.next;
     return head;
    }
}
