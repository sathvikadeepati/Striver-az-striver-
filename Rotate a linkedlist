time-o(n)
space-o(1)
class Solution{
  public ListNode rotatelinkedlist(ListNode head,int k){
    if(head==null || head.next==null || k==0) return head;
     ListNode tail=head;
     int length=1;
     while(tail.next!=null){
      tail=tail.next;
      length++;
     }
    k=k%length;
    if(k==0) return head;
    int steps=length-k;
    ListNode curr=head;
    for(int i=1;i<steps;i++){
      curr=curr.next;
    }
     ListNode newhead=curr.next;
     curr.next=null;
     tail.next=head;
     return newhead;
    }
  }
