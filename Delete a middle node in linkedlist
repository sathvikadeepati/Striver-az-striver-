time-o(N)
space-o(1)
Class Solution{
 public static ListNode deletemiddlenode(ListNode head){
     if(head==null || head.next==null) return null;
     ListNode curr=head;
     int count=0; 
     while(curr!=null){
        count+=1;
        curr=curr.next;
     }
    int mid=count/2;
    curr=head;
    for(int i=0;i<mid-1;i++){
      curr=curr.next;
    }
    curr.next=curr.next.next;
    return head;
  }
}
optimal-time-o(n)
space-o(1)
class Solution{
  public static ListNode deletemiddlenode(ListNode head){
     if(head==null || head.next==null)  return null;
     ListNode slow=head;
     ListNode fast=head.next.next;
     while(fast!=null &&fast.next!=null){
        slow=slow.next;
        fast=fast.next.next;
     }
     slow.next=slow.next.next;
     return head;
  }
}
