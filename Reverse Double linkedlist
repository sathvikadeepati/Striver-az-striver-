time-0(n)
space-O(n)
class Solution{
public static ListNode reverselinkedlist(ListNode head){
  Stack<Integer> st=new Stack<>();
  ListNode curr=head;
  while(curr!=null){
    st.push(curr.val);
    curr=curr.next;
  }
  curr=head;
  while(!st.isEmpty()){
    curr.val=st.pop();
    curr=curr.next;
  }
   return head;
  }
}
optimal
time-o(N)
space-o(1)
class Solution{
  public ListNode reversedoublelinkedlist(ListNode head){
      ListNode curr=head;
      ListNode last=null;
      while(curr!=null){
        last=curr.previous;
        curr.previous=curr.next;
        curr.next=last;
        curr=curr.previous;
      }
      if(last!=null){
        head=last.previous;
      }
     return head;
  }
}
