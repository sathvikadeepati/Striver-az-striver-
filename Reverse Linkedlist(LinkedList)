time-o(n)
space-o(n)
class Solution{
public static ListNode reverselinkedlist(ListNode head){
  Stack<Integer> st=new Stack<>();
  ListNode curr=head;
  while(curr!=null){
    st.push(curr.val);
    curr=curr.next;
  }
  curr=head;
  while(!st.isEmpty()){
    curr.val=st.pop();
    curr=curr.next;
  }
   return head;
  }
}
optimal
time-o(n)
space-o(1)
class Solution{
 public static ListNode reverselinkdlist(ListNode head){
   ListNode prev=null;
   ListNode curr=head;
   while(curr!=null){
     ListNode front=curr.next;
     curr.next=prev;
     prev=curr;
     curr=front;
   }
   return prev;
  }
}
